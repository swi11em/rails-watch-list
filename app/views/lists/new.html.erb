<div class="col-6 m-auto p-5 ">
  <div class="flat-form p-5">
    <h1>Create new list</h1>
    <div class="mt-3">
      <%= simple_form_for(@list) do |f| %>
        <%= f.input :name, input_html: { class: 'form-control' }, label_html: { class: 'form-label' } %>
        <%= f.input :photo, as: :file %>
        <%= f.button :submit, "Create list", class: 'btn btn-primary mt-3' %>
      <% end %>
    </div>
  </div>
</div>
